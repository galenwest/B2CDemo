<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TopBar">
        <attr name="my_title" format="string" />
        <attr name="my_titleTextColor" format="color" />
        <attr name="my_titleLeftImg" format="reference|color" />
        <attr name="leftImgSrc" format="reference|color" />
        <attr name="rightImgSrc" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="TopBarSon">
        <attr name="oneImgSrc" format="reference|color" />
        <attr name="logoImgSrc" format="reference|color" />
        <attr name="threeImgSrc" format="reference|color" />
        <attr name="foreImgSrc" format="reference|color" />
        <attr name="foreVisibility" format="enum" >
            <enum name="visible" value="0" />
            <enum name="invisible" value="1" />
            <enum name="gone" value="2" />
        </attr>
    </declare-styleable>
</resources>